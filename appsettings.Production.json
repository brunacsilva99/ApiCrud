{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5001",
        "Certificate": {
          "Path": "/etc/ssl/certs/aspnetcore.crt",
          "KeyPath": "/etc/ssl/private/aspnetcore.key"
        }
      }
    }
  }

}
